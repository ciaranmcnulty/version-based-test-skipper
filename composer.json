{

    "name": "ciaranmcnulty/versionbasedtestskipper",
    "description": "Skip Behat scenarios based on PHP version",
    "require": {
        "php" : "^7.2 || ^8.0",
        "composer/semver": "~1.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ciaran McNulty",
            "email": "mail@ciaranmcnulty.com"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "": "src"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "^6.0",
        "behat/behat" : "^3.0",
        "phpunit/phpunit" : "^9.0"
    }
}
